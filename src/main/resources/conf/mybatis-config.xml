        <setting name="logImpl" value="LOG4J2"/>
    </settings>
    <!-- 命名空间 -->
    <typeAliases>
    	<typeAlias alias="Article" type="com.henry.blog.model.Article"/>
    </typeAliases>
    <mappers>
    </mappers>

</configuration>
